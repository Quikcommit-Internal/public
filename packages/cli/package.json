{
  "name": "@quikcommit/cli",
  "version": "4.2.0",
  "description": "AI-powered conventional commit messages",
  "bin": {
    "qc": "./dist/index.js"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "git",
    "commit",
    "ai",
    "conventional-commits",
    "cli"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Quikcommit-Internal/public"
  },
  "scripts": {
